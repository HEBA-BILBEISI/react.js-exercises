class StopWatch {
    constructor(hour, mins, sec) {
        this.hour = hour;
        this.mins = mins;
        this.sec = sec;
    }
}
NowTime = new StopWatch(00, 00, 00);

function lab() {
    NowTime.hour++;
    NowTime.mins++;
    
}





